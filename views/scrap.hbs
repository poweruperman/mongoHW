<div class='container'>
    <div class='header'>
        <div class='title'>Mongo Scraper</div>
        <button class='Btn' id='HomeBtn'>
            <a href="https://intense-badlands-32581.herokuapp.com/">
                Home
            </a>
        </button>
        <button class='Btn' id='SavedBtn'>
            <a href="https://intense-badlands-32581.herokuapp.com/saved">
                Saved Articles
            </a>
        </button>
        <button class='Btn' id='ScrapBtn'>
            <a href="https://intense-badlands-32581.herokuapp.com/scrap">
                Scrap New Article
            </a>
        </button>
        <button class='Btn' id='ClearBtn'>
            <a href="https://intense-badlands-32581.herokuapp.com/">
                Clear Articles
            </a>
        </button>
    </div>
    <div class='body'>
        {{#each tmp}}
        <div class='articleBody'>
            <div class='articleTitle' data-index={{index}} id='Title{{index}}'>
                <a href={{url}} id='Title{{index}}Content' data-url={{url}}>{{title}}</a>
            </div>
            <div class='articleSummary' data-index={{index}} id='Summary{{index}}'>
                {{summary}}
            </div>
            <button class='articleBtn' data-index={{index}} id='ArticleBtn{{index}}'>
                Save Article
            </button>
        </div>
        {{/each}}
    </div>
    <div class='footer'>React JS Nnews Scraper using Express-Handlebars</div>
</div>